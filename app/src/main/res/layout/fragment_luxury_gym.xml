<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FamilyGymFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/unterschiede_02"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView40"
                android:layout_width="175dp"
                android:layout_height="39dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="24dp"
                android:text="Luxury Gym"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/imageView30"
                app:layout_constraintTop_toTopOf="@+id/imageView30" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="360dp"
                android:layout_height="287dp"
                android:layout_marginTop="40dp"
                android:text="Ein Wort wie ein Versprechen. Es klingt nach Entspannung auf Fünf-Sterne-Niveau. Genauso sehen sie auch aus, die neuen Wellnessclubs der Luxusklasse, die mit traditionellen Fitnessstudios nur wenig gemeinsam haben. In diesen Lifestyle-Tempeln relaxt man in stylischem Ambiente auf Designersofas. Und genießt das zarte Aroma, das edle Duftkerzen verbreiten. Die Mitglieder trainieren an modernsten Geräten, feilen mit der Ernährungsberaterin an ihrem Speiseplan, gönnen sich im Gourmetrestaurant einen Snack und lassen sich im Saunabereich verwöhnen – mit Spa-Treatments von der Hamam- bis zur Ayurveda-Massage. In London wurde das Konzept der edlen Spa- und Health-Gyms erfunden. Wir stellen die fünf deutschen Top-Adressen vor."
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.49"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView40" />

            <ImageButton
                android:id="@+id/imageButtonBack2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:src="@drawable/ic_baseline_arrow_back_24"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.797"
                app:layout_constraintStart_toEndOf="@+id/textView40"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="359dp"
                android:layout_height="290dp"
                android:layout_marginTop="284dp"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop"
                android:src="@drawable/luxury_gym01"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView5" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</layout>